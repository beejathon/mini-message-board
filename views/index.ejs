<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% messages.forEach(message => { %>
      <div>
        <h2><%= message.user %></h2>
        <p><%= message.text %></p>
        <p><%= message.added %></p>
      </div>
    <% }) %>
    <a href="/new">Create new Post</a>
    <p><%= key %></p>
  </body>
</html>
